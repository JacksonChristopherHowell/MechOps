int x, x1, y, y1;

void setup(){
  size(800,800);
  background (0);  
  x = width/4;
  x1 = width - x;
  y = height/4;
  y1 = height - y;
}

void draw(){
  
    fill(255); // can change depending on mouse position
  ellipse(x,y,100,100);
    fill(255); // can change depending on mouse position
  ellipse(x1,y, 100,100);
    fill(255); // can change depending on mouse position
  ellipse(x, y1, 100,100);
    fill(255); // can change depending on mouse position
  ellipse(x1,y1,100,100);
}
